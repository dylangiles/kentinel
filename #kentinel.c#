#include <stdio.h>

#define MAX_LENGTH 80

int main(int argc, char *argv[]) {
	printf("Kentinel Password Manager\nEnter a command to get started: \n");

	char line[256];

	if(fgets(line, sizeof line, stdin) != NULL) {
		if(line == "view") {
			view();
		}
	}
	
	return 0;
}

void view() {
	printf("View command was used\n");
}
